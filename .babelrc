{
    "presets": [
        "env",
        "react",
        "stage-1"
    ],
    "env": {
        "development": {
            "presets": [
                "react-hmre"
            ]
        },
        "production": {
            "plugins": [
                "transform-react-constant-elements",
                "transform-react-remove-prop-types"
            ]
        }
    },
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
    ]
}
