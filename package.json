{
  "name": "react-redux-dashboard",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "prestart": "npm run remove:dist",
    "start": "npm run transpile && node transpiled/devServer.js",
    "transpile": "babel tools -d transpiled",
    "remove:dist": "rimraf ./dist",
    "clean:dist": "npm run remove:dist && mkdir dist",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "lint": "esw src --color",
    "lint:watch": "npm run lint -- --watch",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prebuild": "npm run clean:dist",
    "build": "npm run transpile && node transpiled/build.js",
    "build:serve": "npm run build && npm run open:dist",
    "open:dist": "node transpiled/distServer.js",
    "deps:check": "ncu",
    "outdated": "npm outdated",
    "deps:update": "ncu -u -a",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r",
    "release:patch": "npm version patch -m \"chore(release): Bump to %s\"",
    "release:minor": "npm version minor -m \"chore(release): Bump to %s\"",
    "release:major": "npm version major -m \"chore(release): Bump to %s\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hugofqueiros/react-redux-dashboard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hugofqueiros/react-redux-dashboard/issues"
  },
  "homepage": "https://github.com/hugofqueiros/react-redux-dashboard#readme",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3",
    "yarn": ">=0.18.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testRegex": "test/.*\\.test\\.js$"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "font-awesome": "4.7.0",
    "highcharts": "^5.0.7",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "normalize.css": "^5.0.0",
    "raven-js": "^3.9.2",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-date-picker": "^5.3.28",
    "react-dates": "6.0.0",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-raven-middleware": "^1.2.0",
    "redux-thunk": "^2.2.0",
    "urijs": "^1.18.5"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "better-npm-run": "^0.0.14",
    "browser-sync": "^2.18.12",
    "chalk": "^1.1.3",
    "compression-webpack-plugin": "^0.3.2",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.11.16",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "david": "^10.1.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.26.0",
    "image-webpack-loader": "^3.2.0",
    "imagemin-webpack-plugin": "^1.4.4",
    "jest-cli": "^18.1.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "node-sass": "^4.3.0",
    "npm-check-updates": "^2.8.9",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dev-utils": "^0.4.2",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-immutable-state-invariant": "^1.2.4",
    "redux-logger": "^2.7.4",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-validator": "^2.3.0"
  }
}
